{
  "name": "arm092/omnipay-telcell",
  "type": "library",
  "description": "Telcell gateway for Omnipay payment processing library",
  "keywords": [
    "gateway",
    "merchant",
    "omnipay",
    "pay",
    "payment",
    "telcell",
    "purchase"
  ],
  "homepage": "https://github.com/arm092/omnipay-telcell",
  "license": "MIT",
  "authors": [
    {
      "name": "Arman Khachatryan",
      "email": "arman.khachatryan@apricode.am"
    }
  ],
  "autoload": {
    "psr-4": {
      "Omnipay\\Telcell\\": "src/"
    }
  },
  "require": {
    "php": "^7.2|8.*",
    "php-http/client-implementation": "^1",
    "php-http/message": "^1.5",
    "php-http/discovery": "^1.14",
    "symfony/http-foundation": "^2.1|^3|^4|^5|^6",
    "moneyphp/money": "^3.1|^4.0.3"
  },
  "require-dev": {
    "omnipay/tests": "^4.1",
    "php-http/mock-client": "^1",
    "php-http/guzzle7-adapter": "^1",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "suggest": {
    "league/omnipay": "The default Omnipay package provides a default HTTP Adapter."
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
